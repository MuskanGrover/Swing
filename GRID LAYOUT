import javax.swing.*;
import java.awt.*;
class frame
{
    frame()
    {

    JFrame frame=new JFrame();
    frame.setSize(500,500);
    frame.setLayout(new GridLayout(3,3,10,10));


    JButton button =new JButton("1");
    JButton buttont=new JButton("2");
    JButton buttonk =new JButton("3");
    JButton buttonm =new JButton("4");
    frame.add(button);
    frame.add(buttont);
    frame.add(buttonk);
    frame.add(buttonm);
    frame.setVisible(true);
    }

public static void main(String[] args)
{
    frame ft=new frame();
}
}
