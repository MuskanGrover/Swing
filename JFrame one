import javax.swing.*;
class frame{
    frame()
    {
        JFrame f1=new JFrame("FIRST FRAME");
        f1.setVisible(true);
        f1.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        f1.setBounds(150,50,800,800);

    }
    public static void main(String[] args)
    {
        frame obj=new frame();
    }

}
